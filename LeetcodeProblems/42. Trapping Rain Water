class Solution:
    def trap(self, height: List[int]) -> int:
        n=len(height)
        if n>=1 and n<=(2*(10**4)):
            maxx=n
            res=0
            arr1=[0]*maxx
            arr2=[0]*maxx
            arr1[0]=height[0]
            for i in range(1,n):
                arr1[i]=max(arr1[i-1],height[i])
            arr2[n-1]=height[n-1]
            for i in range(n-2,-1,-1):
                arr2[i]=max(height[i],arr2[i+1])
            for i in range(n):
                x=min(arr1[i],arr2[i])
                z=x-height[i]
                res=res+z
            return(res)
